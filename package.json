{
  "name": "react-transform-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnadrshin/dashboard.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "dimanklg",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0-beta4",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "enzyme": "^2.6.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "jest": "^17.0.3",
    "react-test-renderer": "^15.4.1",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.3.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "chart.js": "^2.4.0",
    "expect": "^1.20.2",
    "react": "^15.4.1",
    "react-chartjs-2": "^1.1.7",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "whatwg-fetch": "^1.0.0"
  }
}
